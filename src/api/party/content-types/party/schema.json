{
  "kind": "collectionType",
  "collectionName": "parties",
  "info": {
    "singularName": "party",
    "pluralName": "parties",
    "displayName": "Party",
    "name": "party",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "template": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::party-template.party-template"
    },
    "points": {
      "type": "biginteger"
    },
    "seats": {
      "type": "integer",
      "default": 0
    },
    "politicians": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::politician.politician",
      "mappedBy": "party"
    },
    "effectiveness": {
      "type": "decimal"
    },
    "effectiveness_mod": {
      "type": "float"
    },
    "budget": {
      "type": "biginteger"
    },
    "expenses": {
      "type": "biginteger"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "ready_for_election": {
      "type": "boolean",
      "default": false
    }
  }
}
